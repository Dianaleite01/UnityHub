@model UnityHub.Models.Candidaturas

@{
    ViewData["Title"] = "Create";
}

<h2>Create</h2>

<h4>Candidaturas</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
